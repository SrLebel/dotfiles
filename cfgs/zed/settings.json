{
  "icon_theme": "Catppuccin Mocha",
  "features": {
    "edit_prediction_provider": "none"
  },
  "project_panel": {
    "dock": "right"
  },
  "terminal": {
    "dock": "bottom"
  },
  "outline_panel": {
    "dock": "left"
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "autosave": "off",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "vim_mode": true,
  "toolbar": {
    "quick_actions": false
  },
  "format_on_save": "on",
  "tab_size": 2,
  "soft_wrap": "editor_width",
  "theme": {
    "mode": "dark",
    "light": "Ayu Light",
    "dark": "Ayu Dark"
  }
}
